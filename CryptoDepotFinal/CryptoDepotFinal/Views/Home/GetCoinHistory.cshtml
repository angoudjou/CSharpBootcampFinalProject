@model IEnumerable<CryptoDepotFinal.Models.CoinHistory>
@ViewBag.Title 
<h2>History of @ViewBag.Name</h2>
<!--//   -->
<table class=" table table-striped table-hover table-bordered">
    <tr>
        <th>

            Timestamp

        </th>

        <th>

            Value

        </th>
        <th>

            Currency

        </th>
        <th>

            When

        </th>
        
    </tr>


    @foreach (var act in Model)
    {
    <tr>
        <td>

            @Html.DisplayFor(itemmodel => act.timestamp)

        </td>

        <td>

            @Html.DisplayFor(itemmodel => act.value)

        </td>
        <td>

            @Html.DisplayFor(itemmodel => act.currency)

        </td>
        <td>

            @Html.DisplayFor(itemmodel => act.when)

        </td>
       
    </tr>
    }
</table>
